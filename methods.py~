#!/bin/python3.6
# -*-coding:Utf-8 -*

import math

def fonc(x):
    produit = 1.0
    k = 0

    if (x == 0):
        return (1)
    while (k < x):
        produit = produit * (math.sin(x/(2*k+1))/(x/(2*k+1)))
        k += 1
    return produit

def dif_with_pi(value):
    dif = value - (math.pi/2)
    print('diff =', '%.10f' % dif)

def rect(a, h):
    s = 0.0
    i = 0
    while (i < 9999):
        s += (fonc(a + i*h) * h)
        i += 1
    print('Rectangles:')
    print('I%d' % a, '=', '%.10f' % s)
    dif_with_pi(s)
